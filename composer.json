{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "backpack/backupmanager": "^3.0",
        "backpack/base": "^1.1",
        "backpack/crud": "^4.1",
        "backpack/generators": "^3.1",
        "backpack/logmanager": "^4.0",
        "backpack/pagemanager": "^3.0",
        "backpack/permissionmanager": "^6.0",
        "backpack/settings": "^3.0",
        "coffeecode/optimizer": "^2.0",
        "doctrine/dbal": "^2.13",
        "doctrine/inflector": "^2.0",
        "fideloper/proxy": "^4.4",
        "filp/whoops": "^2.12",
        "intervention/image": "^2.7",
        "itsgoingd/clockwork": "^5.0",
        "laravel/framework": "^6.20.26",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "lucascudo/laravel-pt-br-localization": "^1.1",
        "maatwebsite/excel": "^3.1",
        "owen-it/laravel-auditing": "^10.0",
        "pusher/pusher-php-server": "^6.1",
        "spatie/laravel-permission": "^5.3",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "facade/ignition": "^1.16.15",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        },
        "process-timeout": 900
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
